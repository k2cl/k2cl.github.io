@charset "UTF-8"

// base
// -----------------------------------------
@import "base/normalize"
@import "base/flex-grid-framework"

// configs
// -----------------------------------------
cor-principal = #fab729
slider-controler = #fab729

// layout
// -----------------------------------------
@import "layout/header"
@import "layout/banner"
@import "layout/footer"
@import "layout/form"

/* entypo */
[class*="entypo-"]:before
	font-family: 'entypo', sans-serif;
	font-size 24px
	font-weight 300



// global
// -----------------------------------------
body
	font-family 'Roboto', sans-serif
	background url('../img/back.png') repeat

.row
	row()

.row-som
	row()
	justify(right)
	margin-bottom 3em







#header-profile img {
	border-radius: 50%;
	margin: 20px auto;
	display: block;
	width: 210px
	border: 5px solid #fff;
}
.desc
	font-size 20px;
	text-align center
aside {
	border-top: 0px solid #26A69A;
	border-bottom: 0px solid #00695C;
	border-radius: 50%;
	margin: 0 auto;
	margin-top 3em
	display: block;
	height: 250px;
	width: 250px;
	background: url('../img/bg-profile.jpg')
	background-size: cover;
	overflow: hidden;
	box-shadow: 0 0 100px 10px #fff;
	transition: all ease 0.3s;
}

aside:hover {
	border-top: 4px solid #fc9700;
	border-bottom: 4px solid #fc9700;
	border-radius: 5px;
	height: 610px;
	width: 350px;

	box-shadow: 0 0 70px 10px #fff;
}

aside:hover #header-profile img {
	animation: profile_image 2000ms linear both;
	animation-delay: 0.5s;
}

#header-profile {
	text-align: center;
}

#header-profile h1 {
	position: relative;
	text-align: center;
	color: #fff;
	text-shadow: 1px 1px rgba(0, 0, 0, 0.5);
	font-size: 25px;
	line-height: 25px;
	display: inline-block;
	padding: 10px;
	transition: all ease 0.250s;
	border-top: 1px solid #fff;
	border-bottom: 1px solid #fff;
}

aside:hover #header-profile h1 {
	margin-top: 0px;
	outline: 0 solid #fff;
	border-top: 0px solid #fff;
	border-bottom: 1px solid #fff;
}

#header-profile h2 {
	text-align: center;
	color: #fff;
	text-shadow: 1px 1px rgba(0, 0, 0, 0.5);
	font-size: 17px;
	font-weight: normal;
	line-height: 0px;
	margin: 0;
}

.profile-bio {
	margin-top: 20px;
	padding: 1px 20px 10px 20px !important;
	transition: all linear 1.5s;
	color: #fff;
	font-size: 14px;
	opacity: 0;
	background: linear-gradient(to bottom, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.42) 49%, rgba(0, 0, 0, 0.61) 100%);
	}

aside:hover .profile-bio {
	opacity: 1;
}

.profile-bio p:first-child {
	text-align: center;
	font-size: 16px;
}

.profile-social-links {
	position: relative;
	margin-top: -540px;
	margin-left: -100px;
	list-style-type: none;
	opacity: 0;
	transition: all ease 0.5s;
}

aside:hover .profile-social-links {
	margin-left: -25px;
	opacity: 1;
}

.profile-social-links li img {
	width: 30px;
	background: #fff;
	border-radius: 50%;
	padding: 5px;
}
